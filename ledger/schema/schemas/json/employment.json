{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Employment",
  "type": "object",
  "properties": {
    "company": {
      "type": "string"
    },
    "jobTitle": {
      "type": "string"
    },
    "position": {
      "type": "string"
    },
    "startDate": {
      "type": "string",
      "format": "date"
    },
    "endDate": {
      "type": "string",
      "format": "date"
    },
    "emailAddress": {
      "type": "string",
      "format": "email"
    },
    "displayName": {
      "type": "string"
    }
  },
  "required": [
    "company",
    "jobTitle",
    "position",
    "startDate"
  ],
  "additionalProperties": false
}
