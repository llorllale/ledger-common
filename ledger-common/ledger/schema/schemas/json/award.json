{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Award",
  "type": "object",
  "properties": {
    "emailAddress": {
      "type": "string",
      "format": "email"
    },
    "displayName": {
      "type": "string"
    },
    "awardTitle": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "awardDate": {
      "type": "string",
      "format": "date"
    },
    "issuer": {
      "type": "string"
    }
  },
  "required": [
    "awardTitle",
    "awardDate",
    "issuer"
  ],
  "additionalProperties": false
}
